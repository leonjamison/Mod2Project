<h1>Queens Shelters</h1>
<%= video_tag "video.mp4", autoplay: :autoplay, loop: :loop, muted: :muted, playsinline: :playsinline, class: "home_video" %>

<% Shelter.all.each do |s| %>
<% if s.location == 'Queens' %>
<h3>Name: <%= link_to s.name, s %></h3> <br>
<% end %>
<% end %>

<p><%= link_to  "View All Shelters", shelters_path %></p>